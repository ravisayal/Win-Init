CREATE TABLE  HUFND.HUWF_NOTIFICATION_HIST
   (	"RECORD_ID" NUMBER(6,0) GENERATED ALWAYS AS IDENTITY MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 1 CACHE 20 NOORDER  NOCYCLE  NOKEEP  NOT NULL ENABLE, 
	"REPORT_DATE" DATE, 
	"SYSTEM" VARCHAR2(50), 
	"MAILER" VARCHAR2(50), 
	"NOTIFICATION" VARCHAR2(50), 
	"MSG_STATE" VARCHAR2(50), 
	"MSG_COUNT" NUMBER(6,0), 
	 CONSTRAINT "WF_NOTIFICATION_HIST_PK" PRIMARY KEY ("RECORD_ID")
  USING INDEX  ENABLE
   )
/

grant select, insert, update, delete on HUFND.HUWF_NOTIFICATION_HIST to huebs,rsayal;


select *
from hufnd.HUWF_NOTIFICATION_HIST
   <uniqueName>IdeConnections%23OFPI-P3-TNSselect  ap.segment1, ap.vendor_name, hps.party_site_name, (select   --listagg(vendor_site_code , ',') within group (order by vendor_site_code) vnd_sites
                                                                    count(1)
                                                                    --sum(length(vendor_site_code))
                                                            from     ap_supplier_sites_all aps
                                                            where    aps.vendor_id = ap.vendor_id
                                                            ) supp_sites,
        ap.vendor_id, hps.*
from    hz_party_sites hps,
        ap_suppliers ap,
        hz_parties hp
where   1  =  1
and     hps.party_id = hp.party_id
and     hp.party_id = ap.party_id
and     exists 
           (select  1 
           from  ap_supplier_sites_all aps
           where 1=1
           and   aps.party_site_id = hps.party_site_id
           )
and  party_site_name not in 
          (select vendor_site_code
           from  ap_supplier_sites_all aps
           where 1=1
           and   aps.party_site_id = hps.party_site_id
           )
order by 4 asc</uniqueName>
